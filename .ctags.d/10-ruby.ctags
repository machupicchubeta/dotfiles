--regex-ruby=/^[ \t]*alias[ \t]+:([a-zA-Z0-9_!?]+)/\1/l/
--regex-ruby=/^[ \t]*attr_(reader|writer|accessor)[ \t]+:([a-zA-Z0-9_]+)/\2/a/
--regex-ruby=/^[ \t]*class[ \t]+([A-Z][A-Za-z0-9_:]*)/\1/c/
--regex-ruby=/^[ \t]*def[ \t]+([a-zA-Z0-9_!?]+)/\1/f/
--regex-ruby=/^[ \t]*module[ \t]+([A-Z][A-Za-z0-9_:]*)/\1/m/
