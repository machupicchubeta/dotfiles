--regex-ruby=/^[ \t]*alias[ \t]+:([a-zA-Z0-9_!?]+)/\1/alias/
--regex-ruby=/^[ \t]*attr_(reader|writer|accessor)[ \t]+:([a-zA-Z0-9_]+)/\2/attribute/
--regex-ruby=/^[ \t]*class[ \t]+([A-Z][A-Za-z0-9_:]*)/\1/class/
--regex-ruby=/^[ \t]*def[ \t]+([a-zA-Z0-9_!?]+)/\1/method/
--regex-ruby=/^[ \t]*module[ \t]+([A-Z][A-Za-z0-9_:]*)/\1/module/
